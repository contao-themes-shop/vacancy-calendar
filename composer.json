{
  "name": "contao-themes-shop/vacancy-calendar",
  "description": "Contao bundle to show vacancy of different objects in monthly calendars",
  "license": "LGPL-3.0-or-later",
  "type": "contao-bundle",
  "keywords": [
    "contao",
    "vacancy",
    "calendar"
  ],
  "authors": [
    {
      "name": "Christopher Boelter",
      "email": "support@contao-themes-shop.de"
    }
  ],
  "homepage": "https://contao-themes-shop.de/",
  "require": {
    "php": "^8.1",
    "codefog/contao-haste": "^5.1",
    "contao-community-alliance/meta-palettes": "^2.1",
    "contao/core-bundle": "^5.3",
    "doctrine/dbal": "^3.8",
    "nesbot/carbon": "^3.0",
    "netzmacht/contao-toolkit": "^4.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/translation-contracts": "^3.4"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0",
    "phpcq/runner-bootstrap": "^1.x-dev"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemesShop\\VacancyCalendar\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "contao-manager-plugin": "ContaoThemesShop\\VacancyCalendar\\ContaoManager\\Plugin"
  }
}
